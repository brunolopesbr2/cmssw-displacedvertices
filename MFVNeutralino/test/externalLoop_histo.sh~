#!/usr/bin/bash


for line in `cat ntuplesHiggs.txt`; do
    echo $line
    sed 's/INPUTNAME/'$line'/' histos_TEMPLATE.py > histos_TEST.py
    #sed 's/OUTPUTNAME/'$line'/' histos_TEST.py > histos_TEST.py
    cmsRun histos_TEST.py
done

